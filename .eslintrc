{
  "env": {
    "es6": true,
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ],
    "no-nested-ternary": "off",
    "quotes": [
      2,
      "single"
    ],
    "consistent-return": "off",
    "no-underscore-dangle": "off"
  },
  "plugins": [
    "prettier"
  ],
  "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
}